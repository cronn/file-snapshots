{
  "name": "@cronn/element-snapshot",
  "version": "0.7.0",
  "description": "Element snapshots for Playwright",
  "author": "cronn",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cronn/file-snapshots.git",
    "directory": "packages/element-snapshot"
  },
  "homepage": "https://github.com/cronn/file-snapshots",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": "^20 || ^22 || >=24"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24.11 <25",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": ">=10.21 <11",
      "onFail": "error"
    }
  },
  "scripts": {
    "lint:check": "eslint src/ --max-warnings=0",
    "lint:fix": "eslint src/ --max-warnings=0 --fix",
    "compile": "tsc",
    "build": "tsup"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.global.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@cronn/shared-configs": "workspace:*",
    "@trivago/prettier-plugin-sort-imports": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-unused-imports": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  }
}
