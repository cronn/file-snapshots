{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "test": {
      "dependsOn": ["^build", "compile"],
      "outputLogs": "new-only"
    },
    "test:coverage": {
      "dependsOn": ["^build", "compile"],
      "outputs": ["coverage/**"],
      "outputLogs": "new-only"
    },
    "check": {
      "dependsOn": ["compile", "lint:check", "test:coverage", "//#format:check"]
    }
  }
}
